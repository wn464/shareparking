<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.project.dao.ImagesDao"> 
  <insert id="addimages"  useGeneratedKeys="true" keyProperty="images.id" >
  insert into imgs (image1,image2,image3) values(#{images.image1},#{images.image2},#{images.image3});
 
  </insert>
  </mapper>